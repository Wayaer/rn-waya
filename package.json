{
  "name": "rn-waya",
  "version": "0.0.7",
  "description": "React Native rn-waya",
  "main": "index.js",
  "homepage": "https://github.com/Wayaer/rn-waya",
  "author": "Wayaer",
  "scripts": {
    "android755": "chmod 755 android/gradlew",
    "androidBundle": "react-native bundle --platform android --entry-file index.js --reset-cache --bundle-output android/bundle/index.bundle --dev false --assets-dest android/bundle/",
    "iosBundle": "react-native bundle --platform ios --entry-file index.js --reset-cache --bundle-output ios/bundle/index.bundle --dev false --assets-dest ios/bundle/",
    "useNpm": "npx nrm use npm",
    "useTaoBao": "npx nrm use taobao",
    "npmPublish": "npm publish",
    "npmUnPublish": "npm unpublish rn-waya@0.0.6",
    "npmDeprecate": "npm deprecate rn-waya@0.0.1 Please update the version for rn-waya"
  },
  "dependencies": {
    "eslint": "^6.7.2",
    "prop-types": "latest",
    "react-native-svg": "^9.13.6",
    "rn-curiosity": "latest",
    "teaset": "latest"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^0.0.5"
  },
  "peerDependencies": {
    "@react-native-community/async-storage": "latest",
    "@react-native-community/netinfo": "latest",
    "react-native-svg": "latest",
    "rn-curiosity": "latest",
    "rn-fetch-blob": "latest"
  },
  "prettier": {
    "requirePragma": true,
    "proseWrap": "always",
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "semi": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "parser": "flow",
    "jsxBracketSameLine": true
  },
  "eslintConfig": {
    "extends": "@react-native-community"
  }
}
