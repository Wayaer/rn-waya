{
  "name": "rn-waya",
  "version": "0.0.7",
  "description": "React Native rn-waya",
  "main": "index.js",
  "homepage": "https://github.com/Wayaer/rn-waya",
  "license": "license",
  "author": "Wayaer",
  "scripts": {
    "android755": "chmod 755 android/gradlew",
    "androidBundle": "react-native bundle --platform android --entry-file index.js --reset-cache --bundle-output android/bundle/index.bundle --dev false --assets-dest android/bundle/",
    "iosBundle": "react-native bundle --platform ios --entry-file index.js --reset-cache --bundle-output ios/bundle/index.bundle --dev false --assets-dest ios/bundle/",
    "npmPublish": "npm publish",
    "npmUnPublish": "npm unpublish wayae@0.0.6",
    "npmDeprecate": "npm deprecate wayae@0.0.0 Please update the version for wayae"
  },
  "keywords": [
    "react-native"
  ],
  "dependencies": {
    "eslint": "^6.1.0",
    "prop-types": "latest",
    "rn-curiosity": "latest",
    "teaset": "0.6.3"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^0.0.5"
  },
  "peerDependencies": {
    "react-navigation": ">=3.3.0",
    "rn-fetch-blob": ">=0.10.16",
    "@react-native-community/async-storage": ">=1.5.1",
    "@react-native-community/netinfo": ">=4.1.3",
    "rn-curiosity": "latest"
  },
  "prettier": {
    "requirePragma": true,
    "proseWrap": "always",
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "semi": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "parser": "flow",
    "jsxBracketSameLine": true
  },
  "eslintConfig": {
    "extends": "@react-native-community"
  }
}
